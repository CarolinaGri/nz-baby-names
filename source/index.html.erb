<script id='locale_tmpl' type='text/ejs'>
  <a href='#' class='locale-btn'>
    <img class='locale-btn__flag' src='img/<%%= code %>.svg' />
    <%%= name %>
  </a>
</script>

<script id='locales_tmpl' type="text/ejs">
  <div class="btn-group">
    <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
      <img class='locale-btn__flag' src='img/<%%= code %>.svg' />
      <%%= name %>
      <span class="caret"></span>
    </button>
    <ul class="dropdown-menu" role="menu"></ul>
  </div>
</script>

<script id='gender_tmpl' type='text/ejs'>
  <a href='#' class='btn <%%= buttonClass() %>'>
    <i class='icon-<%%= icon %>'></i>
    <%%= name %>
  </a>
</script>
